    "# day 1\n",
    "f = open(\"data/data01.txt\", \"r\")\n",
    "lines = f.readlines()\n",
    "lines = [l[:-1] for l in lines]\n",
    "results = []\n",
    "a = 0\n",
    "for l in lines:\n",
    "    if len(l)==0:\n",
    "        results.append(a)\n",
    "        a = 0\n",
    "    else:\n",
    "        a += int(l)\n",
    "\n",
    "print(max(results))\n",
    "print(sum(sorted(results)[-3:]))"
   ]
